<p>
  <a class="btn btn-info" href="/lists/<%= params[:list_name] %>/new">New User</a>
</p>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th><%= user["uuid"] %></th>
        <td><%= user["name"] %></td>
        <td><%= user["email"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
